{
  "AppSettings": {
    "Secret": "{{ cookiecutter.appSettings_production.secret }}",
    "RefreshTokenTTL": {{ cookiecutter.appSettings_production.refreshTokenTTL }},
    "ServerMail": {
      "Host": "{{ cookiecutter.appSettings_production.serverMail.host }}",
      "Port": {{ cookiecutter.appSettings_production.serverMail.port }},
      "SSL": {{ cookiecutter.appSettings_production.serverMail.ssl }},
      "Login": "{{ cookiecutter.appSettings_production.serverMail.login }}",
      "Password": "{{ cookiecutter.appSettings_production.serverMail.password }}",
      "Email": "{{ cookiecutter.appSettings_production.serverMail.email }}",
      "Name": "{{ cookiecutter.appSettings_production.serverMail.name }}"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "{{ cookiecutter.connectionStrings_production.defaultConnection }}",
    "RedisConnection": "{{ cookiecutter.connectionStrings_production.redisConnection }}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
